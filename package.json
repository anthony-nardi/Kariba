{
  "name": "@anthonynardi/familyinc",
  "private": true,
  "version": "1.0.0",
  "author": "Anthony Nardi",
  "license": "MIT",
  "workspaces": {
    "packages": [
      "workspaces/*"
    ]
  },
  "scripts": {
    "start:dev:client": "yarn workspace @familyinc/client start:dev",
    "start:dev:server": "yarn workspace @familyinc/server start:dev",
    "start:prod:client": "yarn workspace @familyinc/client start:prod",
    "start:prod:server": "yarn workspace @familyinc/server start:prod",
    "build": "yarn workspaces foreach run start:prod",
    "build:dev": "yarn workspaces foreach run start:dev"
  },
  "packageManager": "yarn@3.2.4"
}
